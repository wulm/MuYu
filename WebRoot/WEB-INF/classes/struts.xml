<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="JC_Web_System" namespace="/" extends="struts-default">
		<action name="sysUser" class="userAction">
			<result name="loginSuccess">/WeixinPages/articleManage.jsp</result>
			<result name="loginFail">/login.jsp</result>
		</action>
		<action name="article" class="articleAction">
		
			<result name="articleEditList">/WeixinPages/articleEditList.jsp</result>
			<result name="articleManage">/WeixinPages/articleManage.jsp</result>
			<result name="editArticle">/WeixinPages/editArticle.jsp</result>
			<result name="articleList">/WeixinPages/articleList.jsp</result>
			<result name="articleDetail">/WeixinPages/articleDetail.jsp</result>
		</action>
	</package>
</struts>
